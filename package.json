{
  "name": "portfolio",
  "private": true,
  "description": "Nick's Portfolio WIP",
  "version": "0.1.0",
  "author": "Nick Petalas <webmasternikos@gmail.com>",
  "dependencies": {
    "autoprefixer": "10.4.20",
    "gatsby": "5.13.6",
    "gatsby-plugin-gatsby-cloud": "5.13.1",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-offline": "6.13.2",
    "gatsby-plugin-react-helmet": "6.13.1",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "tailwindcss": "3.4.10"
  },
  "devDependencies": {
    "gatsby-plugin-postcss": "6.13.1",
    "gh-pages": "6.1.1",
    "prettier": "3.3.3"
  },
  "keywords": [
    "petalas",
    "portfolio"
  ],
  "scripts": {
    "build": "gatsby build",
    "lint": "prettier --check \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "add-domain": "echo \"petalas.dev\" > ./public/CNAME",
    "deploy": "npm run clean && npm run build && npm run add-domain && gh-pages -d public -b deploy-branch -f"
  }
